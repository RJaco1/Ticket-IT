<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.ticket_it.ticketit">

    <uses-sdk tools:overrideLibrary="com.google.zxing.client.android" />

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:name="androidx.multidex.MultiDexApplication"
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/entradas"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/entradas"
        android:supportsRtl="true"
        android:theme="@style/Theme.TicketIt">
        <activity android:name=".Turno" />
        <activity android:name=".HistorialTurno" />
        <activity android:name=".AgendaTurno" />
        <activity android:name=".QrTurno" />
        <activity android:name=".ReservaTurno" />
        <activity android:name=".MenuEntidad" />
        <activity android:name=".MenuTipoEntidad" />
        <activity android:name=".MenuPrincipal"/>
        <activity android:name=".Login" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Form" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".CaptureAct"
            android:screenOrientation="fullSensor"
            android:stateNotNeeded="true"
            android:windowSoftInputMode="stateAlwaysHidden"
            tools:replace="screenOrientation" />
    </application>

</manifest>